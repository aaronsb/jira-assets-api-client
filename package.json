{
  "name": "jira-assets-api-client",
  "version": "1.0.0",
  "description": "A Node.js library that automatically generates a TypeScript client for the Atlassian Assets API from the OpenAPI specification",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "clean:generated": "rimraf src/generated assets-openapi.json",
    "prebuild": "npm run clean",
    "prepare": "npm run build",
    "start": "ts-node src/index.ts",
    "download": "ts-node src/downloadAssetsApiSpec.ts",
    "generate": "ts-node src/generateAssetsApiClient.ts",
    "fix": "ts-node src/fixGeneratedCode.ts",
    "postgenerate": "npm run fix",
    "example": "ts-node examples/basic-usage.ts",
    "example:advanced": "ts-node examples/advanced-usage.ts",
    "reset": "npm run clean && npm run clean:generated"
  },
  "keywords": [
    "jira",
    "assets",
    "api",
    "openapi",
    "typescript",
    "client",
    "generator"
  ],
  "author": "",
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "axios": "^1.8.4",
    "dotenv": "^16.4.7",
    "openapi-typescript-codegen": "^0.29.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
